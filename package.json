{
  "name": "google-search-datepicker",
  "version": "0.0.5",
  "license": "MIT",
  "author": "Hideki Ikemoto",
  "scripts": {
    "archive": "ts-node scripts/archive.ts",
    "build": "webpack",
    "deploy": "ts-node scripts/github_release.ts",
    "format": "pnpm format:prettier && pnpm format:sort-package-json",
    "format:check": "pnpm format:check:prettier && pnpm format:check:sort-package-json",
    "format:check:prettier": "prettier --check .",
    "format:check:sort-package-json": "sort-package-json --check",
    "format:prettier": "prettier --write .",
    "format:sort-package-json": "sort-package-json",
    "lint": "pnpm eslint --ext .ts apps test *.ts",
    "lint:ci": "pnpm format:check && pnpm eslint --format junit --output-file ./reports/eslint/eslint.xml --ext .ts apps test *.ts",
    "manifest": "ts-node scripts/make_manifest.ts",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/chrome": "0.0.241",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.19",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "date-fns": "2.30.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "sort-package-json": "^2.5.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.1",
    "zip-a-folder": "2.0.0"
  },
  "packageManager": "pnpm@8.6.9"
}
